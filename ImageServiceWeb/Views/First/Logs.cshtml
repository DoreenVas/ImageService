@model ImageServiceWeb.Models.LogsModel
@using ImageService.Logging.Modal;

@{
    ViewBag.Title = "Logs";
}

<h2>Logs</h2>
<div>
    <table>
        @foreach (MessageRecievedEventArgs log in Model.Logs)
        {
            <tr>
                <th>
                    @log.Status
                </th>
                <td>
                    @log.Message
                </td>
            </tr>
        }
    </table>
</div>
